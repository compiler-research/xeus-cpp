{
  "display_name": "C++14",
  "env": {
      "PATH":"@XEUS_CPP_PATH@",
      "LD_LIBRARY_PATH":"@XEUS_CPP_LD_LIBRARY_PATH@"
  },
  "argv": [
      "@XEUS_CPP_KERNELSPEC_PATH@xcpp",
      "-f",
      "{connection_file}",
      "-resource-dir", "@XEUS_CPP_RESOURCE_DIR@",
      "-I", "@CMAKE_INSTALL_PREFIX@/include",
      "-std=c++14",
      "-fno-exceptions",
      "-O2",
      "-v"@XEUS_CPP_OMP@
  ],
  "language": "cpp",
  "metadata": {"debugger": false}
}
